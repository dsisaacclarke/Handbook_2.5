Функция Split

#include <vector>
#include <string>

std::vector<std::string> Split(const std::string& str, char delimiter) {
    std::vector<std::string> result;
    std::string token;
    size_t start = 0, end = 0;
    
    while (end <= str.size()) {
        end = str.find(delimiter, start);
        result.push_back(str.substr(start, end - start));
        start = end + 1;
    }
    return result;
}
